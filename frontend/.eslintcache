p/appAcademy/Module5/w16/project/gymbnb/frontend/src/components/ReviewsPage/index.js",["85"],"import './ReviewsPage.css'\nimport { useState, useEffect } from 'react'\nimport { useSelector, connect } from 'react-redux'\nimport { Link } from 'react-router-dom'\nimport ReactStars from 'react-rating-stars-component'\nimport Modal from 'react-modal'\n\nModal.setAppElement('#root')\n\nconst ReviewsPage = ({ reviews }) => {\n    let reviewsPreview = []\n    const user = useSelector(state => state.session.user)\n    const newReview = useSelector(state => state.reviews.newReview)\n    const [modalOpen, setModalOpen] = useState(false)\n\n    const customStyles = {\n        content: {\n            top: '50%',\n            left: '50%',\n            right: 'auto',\n            bottom: 'auto',\n            height: '50vh',\n            width: '50vw',\n            marginRight: '-50%',\n            transform: 'translate(-50%, -50%)',\n            padding: '1rem',\n        }\n    };\n\n    const openModal = () => {\n        setModalOpen(true)\n    }\n\n    const closeModal = () => {\n        setModalOpen(false)\n    }\n\n    if (reviews.length > 4) {\n        for (let i = 0; i < 4; i++) {\n            reviewsPreview.push(reviews[i])\n        }\n    }\n    if (reviews.length < 4 && reviews.length >= 1) {\n        for (let i = 0; i < reviews.length; i++) {\n            reviewsPreview.push(reviews[i])\n        }\n    }\n\n    useEffect(() => {\n        if (modalOpen) {\n            document.body.classList.add('overflow')\n        } else {\n            document.body.classList.remove('overflow')\n        }\n    }, [modalOpen])\n\n\n    return (\n        <div className='reviews-section-container'>\n            <h3 className='reviews-section-header'>\n                Reviews\n                {user && (\n                    <Link to={`/listings/${reviews[0].listingId}/reviews/add`}>\n                        <button\n                            className='show-reviews-button'\n                            style={{ float: 'right' }}\n                        >\n                            Add a review\n                    </button>\n                    </Link>\n                )}\n                {!user && (\n                    <Link to='/login'>\n                        <button\n                            className='show-reviews-button'\n                            style={{ float: 'right' }}\n                        >\n                            Log in to add a review\n                    </button>\n                    </Link>\n                )}\n            </h3>\n            <ul>\n                {reviewsPreview.length >= 1 && (\n                    <>\n                        {reviewsPreview.map(review =>\n                            <div key={`${reviewsPreview.indexOf(review)}`}>\n                                <li>\n                                    <h4 style={{ color: '#FF385C' }}>\n                                        {review.User.username}\n                                        <span style={{ color: 'black' }}>\n                                            {review.createdAt.split('T')[0]}\n                                        </span>\n                                        <ReactStars {...{ size: 15, value: review.starsRating, edit: false }} />\n                                    </h4>\n                                    <p>\n                                        {review.content}\n                                    </p>\n                                </li>\n                            </div>\n                        )}\n                        {reviews.length > 4 && (\n                            <>\n                                <div className='show-reviews-button-container'>\n                                    <button\n                                        className='show-reviews-button'\n                                        onClick={openModal}\n                                    >\n                                        {`Show all ${reviews.length} reviews`}\n                                    </button>\n                                </div>\n                                <Modal\n                                    isOpen={modalOpen}\n                                    onRequestClose={closeModal}\n                                    style={customStyles}\n                                    contentLabel='modal'\n                                >\n                                    <div style={{ borderBottom: '1px solid lightgray' }}>\n                                        <h2 style={{ paddingLeft: '1rem', fontWeight: '500' }}>Reviews</h2>\n                                    </div>\n                                    <div style={{ overflowY: 'auto', }}>\n                                        {reviews.map(review =>\n                                            <div style={{ padding: '1rem' }} key={`${reviews.indexOf(review)}`}>\n                                                <li>\n                                                    <h4 style={{ color: '#FF385C' }}>\n                                                        {review.User.username}\n                                                        <span style={{ color: 'black' }}>\n                                                            {review.createdAt.split('T')[0]}\n                                                        </span>\n                                                        <ReactStars {...{ size: 15, value: review.starsRating, edit: false }} />\n                                                    </h4>\n                                                    <p>\n                                                        {review.content}\n                                                    </p>\n                                                </li>\n                                            </div>\n                                        )}\n                                    </div>\n                                </Modal>\n\n                            </>\n                        )}\n                    </>\n                )}\n                {reviewsPreview.length === 0 && (\n                    <div>\n                        <li>\n                            <p>\n                                No reviews yet\n                            </p>\n                        </li>\n                    </div>\n                )}\n            </ul>\n        </div >\n    )\n}\n\nconst mapStateToProps = (state) => {\n    return { newReview: state.reviews.newReview }\n}\n\nexport default connect(mapStateToProps)(ReviewsPage)\n","/mnt/c/users/sebthtan/desktop/appAcademy/Module5/w16/project/gymbnb/frontend/src/store/reviews.js",[],"/mnt/c/users/sebthtan/desktop/appAcademy/Module5/w16/project/gymbnb/frontend/src/components/AddReviewPage/index.js",[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"86","replacedBy":"90"},{"ruleId":"88","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":13,"column":11,"nodeType":"94","messageId":"95","endLine":13,"endColumn":20},"no-native-reassign",["96"],"no-negated-in-lhs",["97"],["96"],["97"],"no-unused-vars","'newReview' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]